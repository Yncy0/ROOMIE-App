{
  "expo": {
    "name": "ROOMIE App",
    "slug": "ROOMIE_App",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/roomie-icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.example.roomie"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/roomie-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.anonymous.ROOMIE_App"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/roomie-splash-icon.png",
          "backgroundColor": "#ffffff",
          "dark": {
            "image": "./assets/images/roomie-splash-icon.png",
            "backgroundColor": "#000000"
          },
          "imageWidth": 200,
          "resizeMode": "contain"
        }
      ],
      "expo-font",
      "react-native-edge-to-edge"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "4cb9da4b-c5f0-44c5-9780-c3ebff5335d2"
      }
    },
    "owner": "yncy0"
  }
}
